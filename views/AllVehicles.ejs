<%- include('./partials/header') %>
<% vehicles.forEach(vehicle => { %>
    <div class="mb-6 px-6 py-4 border  rounded-lg bg-zinc-100 text-black">
        <p>Type: <%= vehicle.type %></p>
        <p>Model: <%= vehicle.model %></p>
        <p>License Plate: <%= vehicle.licensePlate %></p>
        <p>Status: <%= vehicle.status %></p>
        <a class="w-10 h-7 flex items-center justify-center bg-red-500 rounded-md mt-5 hover hover:bg-green-500 " href="/vehicles/EditVehicle/<%= vehicle._id%>">Edit</a>
    </div>
<% }) %>

<%- include('./partials/footer') %>